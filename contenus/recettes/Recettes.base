filters:
  and:
    - note["base"] == link("Recettes.base")
properties:
  file.name:
    displayName: Nom
  Note:
    displayName: Note
  État:
    displayName: État
  Portions:
    displayName: Portions
  URL:
    displayName: URL
  Tag:
    displayName: Tag
views:
  - type: table
    name: Table View
    order:
      - file.name
      - source
      - tags
    columnSize:
      file.name: 377
  - type: cards
    name: Cards
    order:
      - file.name
